{
  "name": "swizzler-aabb",
  "version": "999.999.999",
  "engines": {
    "node": ">=12.13.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jameswilddev/swizzler-aabb"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "4.3.1",
    "@types/node": "18.15.2",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "del-cli": "5.0.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "4.2.1",
    "jasmine": "4.5.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "prettier": "2.8.4",
    "swizzler": "0.0.5",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "swizzler": "0.0.5"
  },
  "scripts": {
    "buildClean": "del-cli \"**/*.js\" \"**/*.d.ts\" \"!**/node_modules/**/*\"",
    "buildTypeScript": "tsc",
    "buildJasmineUnit": "jasmine --config=unit.jasmine.json",
    "buildNyc": "nyc --check-coverage --all --exclude \"**/*.ts\" --exclude \"**/*.d.ts\" --exclude \"**/unit.*\" --lines 100 --functions 100 --branches 100 --statements 100 npm run-script buildJasmineUnit",
    "test": "npm-run-all buildClean buildTypeScript buildNyc",
    "lint": "eslint \"**/*.ts\""
  },
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "!**/unit.js",
    "!**/unit.d.ts"
  ]
}
